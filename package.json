{
  "name": "datpartout",
  "version": "1.0.0",
  "description": "Parses the PARTOUT(4.0)RESOURCE file format used by Maxis/Cinematronics in Full Tilt Pinball and displays the contents.",
  "private": true,
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "jszip": "*",
    "three": "^0.130.1"
  },
  "devDependencies": {
    "gh-pages": "^3.0.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "dev": "webpack serve --mode=development",
    "build": "webpack --mode=production",
    "predeploy": "del /S /Q dist\\lib && webpack --mode=production",
    "deploy": "gh-pages -d dist"
  },
  "repository": "git+https://github.com/belaw/DatPartoutExplorer.git",
  "author": "Jonas Witting",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/belaw/DatPartoutExplorer/issues"
  },
  "homepage": "https://belaw.github.io/DatPartoutExplorer",
  "packageManager": "yarn@3.1.1"
}
