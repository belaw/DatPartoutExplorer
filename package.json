{
  "name": "datpartout",
  "version": "1.0.0",
  "description": "Parses the PARTOUT(4.0)RESOURCE file format used by Maxis/Cinematronics in Full Tilt Pinball and displays the contents.",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "three": "^0.115.0"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "parcel-bundler": "^1.12.4"
  },
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "predeploy": "del /S /Q dist && parcel build src/index.html --public-url /DatPartoutExplorer",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belaw/DatPartoutExplorer.git"
  },
  "author": "Jonas Witting",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/belaw/DatPartoutExplorer/issues"
  },
  "homepage": "https://belaw.github.io/DatPartoutExplorer"
}
